<section>

    
    <div class="container mt-3">
        <div class="row">
            <a href="/admin/add-Product" class="btn btn-primary  justify-content-end">Add Product</a>
        </div>
        <table class="table mt-4">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Name</th>
                    <th scope="col">Category</th>
                    <th scope="col">Description</th>
                    <th scope="col">Price</th>
                    <th scope="col">Image</th>
                </tr>
            </thead>
            <tbody>
                {{#each Product}}
                <tr>
                    <th scope="row">1</th>
                    <td>{{name}}</td>
                    <td>{{category}}</td>
                    <td>{{description}}</td>
                    <td>nill</td>
                    <td><img src="{{image}}" style="width: 50px; height: 50px;" alt=""></td>
                </tr>
                {{/each}}

            </tbody>
        </table>
    </div>
</section>